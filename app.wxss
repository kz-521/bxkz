page {
  background: #f0eff5;
  height: 100vh;
  font-family: 微软雅黑;
  font-size: 16px;
}

.zhezao {
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0px;
  background: rgba(0, 0, 0, 0.4);
  z-index: 999;
}

.zhezao-down {
  position: fixed;
  height: 100%;
  top: 0px;
  z-index: 1;
}

.t_w {
  position: absolute;
  top: 45%;
  left: 50%;
  transform: translate(-50%, -50%);
}

.vertical_t_w{
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
}

.tanchu_view {
  width: 600rpx;
  overflow: hidden;
  background-color: #fff;
  border-radius: 25rpx;
  padding: 0rpx;
}

.bg_view {
  background: url('https://client.uqbike.com/images/zhezhao/bg.png');
  background-size: 100% 100%;
  height: 400rpx;
}
.bg_view1 {
  
  
  background-size: 100% 100%;
  height: 400rpx;
}

.txtys {
  margin: 20rpx 50rpx 0rpx 50rpx;
  font-size: 40rpx;
  
  white-space: normal;
  color: #585858;
}

.txtsure {
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: space-around;
}

/*确定背景*/

.txtsurebg {
  width: 160rpx;
  height: 85rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 10rpx;
  margin: 40rpx 25rpx 40rpx 25rpx;
  flex-direction: column;
}

/*确定图标*/

.txtsureimg {
  display: block;
  width: 120rpx;
  height: 120rpx;
}

/*确定文本*/

.txtsurename {
  margin-bottom: 0rpx;
  font-size: 38rpx;
}

/*右上角图标*/

.t_image {
  width: 200rpx;
  height: 200rpx;
  position: absolute;
  top: -6%;
  left: 89.5%;
}

/*右上角图标*/

.t_image1 {
  display: block;
  width: 60rpx;
  height: 60rpx;
}

