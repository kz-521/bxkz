   display: flex;
  flex-direction: row;
  justify-content: flex-start; 
} 
.top_box{
  height: 120rpx;
  display: flex;
  justify-content:space-around;
  /* background-color:pink; */
}
.top_left{
  width: 33%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.top_left2{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: left;
